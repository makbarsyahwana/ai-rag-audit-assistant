{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(rag_http_requests_total[5m])",
          "legendFormat": "{{method}} {{path}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "HTTP Request Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(rag_http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95 {{method}} {{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Ingestion Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "rag_ingestion_total",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "title": "Retrieval Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "rag_retrieval_total",
          "legendFormat": "{{mode}}"
        }
      ]
    },
    {
      "title": "Generation Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "rag_generation_total",
          "legendFormat": "{{abstained}}"
        }
      ]
    },
    {
      "title": "Errors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "rag_errors_total",
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "color": { "mode": "fixed", "fixedColor": "red" } }
      }
    },
    {
      "title": "Retrieval Latency by Mode",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "rate(rag_retrieval_duration_seconds_sum[5m]) / rate(rag_retrieval_duration_seconds_count[5m])",
          "legendFormat": "avg {{mode}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Generation Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "rate(rag_generation_duration_seconds_sum[5m]) / rate(rag_generation_duration_seconds_count[5m])",
          "legendFormat": "avg generation"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    }
  ],
